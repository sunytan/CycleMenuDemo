                                    org.apache.axis2:axis2-resource-bundle:${project.version}
                                </resourceBundle>
                            </resourceBundles>
                        </configuration>
                    </execution>
                </executions>
                <configuration>
                    <connectionType>developerConnection</connectionType>
                </configuration>
            </plugin>

            <plugin>
